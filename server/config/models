Message json
  channel ChannelId
  ts Slack.TS
  user UserId
  text Text
  editedUser UserId Maybe
  editedTs Slack.TS Maybe
  UniqueMessage channel ts

MessageAttachment json
  message MessageId
  number Int
  fallback Text
  color Text Maybe
  pretext Text Maybe
  authorName Text Maybe
  authorLink Text Maybe
  authorIcon Text Maybe
  title Text Maybe
  titleLink Text Maybe
  fields (Map Text FieldPair)
  UniqueMessageAttachment message number

MessageReaction json
  message MessageId
  name Text
  count Int
  users [UserId]
  UniqueMessageReaction message name

User json
  Id (Slack.ID Slack.User)
  name Text
  realName Text
  deleted Bool
  color Text
  tz Text
  tzLabel Text
  tzOffset Int
  image72 Text Maybe
  image512 Text Maybe

Channel json
  Id (Slack.ID Slack.Channel)
  name Text
  isArchived Bool
  isGeneral Bool
  topic Text
